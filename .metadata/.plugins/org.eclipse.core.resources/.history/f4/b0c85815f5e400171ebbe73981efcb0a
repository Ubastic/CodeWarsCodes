
public class Main {

	public static void main(String[] args){
		
		String shape = String.join("\n", new String[] {"+------------+",
										               "|            |",
										               "|            |",
										               "|            |",
										               "+------+-----+",
										               "|      |     |",
										               "|      |     |",
										               "+------+-----+"});
		
		System.out.println(BreakPieces.process(shape));
		
	}
	
}

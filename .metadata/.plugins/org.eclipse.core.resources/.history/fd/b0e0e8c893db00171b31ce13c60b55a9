
public class test {

	public static String fillInAndConsecutive(String a, String prev){
		//if matches return calcPosition(Integer.parseInt(a))
		int i = 0;
		while(true){
			if(prev.charAt(i) != 'x')
				break;
			else 
				i++;
		}
		int num = Integer.parseInt(prev.substring(i)) + 1;
		String numStr = Integer.toString(num);
		for(int j = numStr.length() - 1; j >= 0; j--)
			if(a.charAt(j) == 'x' || numStr.charAt(j) != a.charAt(j))
				return "failed";
		String returnStr = a.substring(0,a.length()-numStr.length());
		returnStr += numStr;
		return returnStr;
	}
	
}
